<aura:component controller="AccountListController" implements="flexipage:availableForAllPageTypes,force:appHostable,lightning:isUrlAddressable,lightning:availableForFlowScreens,force:lightningQuickAction">
    <aura:attribute name="accList" type="Account[]" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <lightning:overlayLibrary aura:id="overlayLib" />
    <lightning:card title="Lightning:overlayLibrary" iconName="standard:account">
        <table class="slds-table slds-table_bordered" >
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Edit / Modal</th>
                </tr>
            </thead>
            <tbody>
                <aura:iteration items="{!v.accList}" var="obj">
                    
                    <tr>
                        <td>  {!obj.Name}</td>
                        <td>
                            <lightning:button variant="brand" label="Edit" onclick="{! c.handleClick }" value="{!obj.Id}" /> <br/>
                        </td>
                    </tr>
                </aura:iteration>    
                
            </tbody>
        </table>
    </lightning:card>
</aura:component>